/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * Common styles
 */

.dataTables_wrapper table { width: 100%; background-color: transparent; margin: 0; padding: 0; border-collapse:collapse; border:0px; }
.dataTables_wrapper tr.datarow, .odd { background-color: #f9f9f9; }
.dataTables_wrapper tr.alternaterow, .even { background-color: #fff; }
.dataTables_wrapper tr.trmouseover { background-color: #daeaf8 !important; }
.dataTables_wrapper th,.dataTables_wrapper td { padding:8px 0px; border-right: 1px solid #ddd;  text-align: center; color: #666;  font-size: 12px;}
.dataTables_wrapper th { font-weight:bold;  cursor:pointer; }
*.dataTables_wrapper th {  border-bottom: 1px solid #ddd;}
.dataTables_wrapper td { border-bottom: 1px solid #ddd; color:#6e6e6e; empty-cells:show;}
.dataTables_wrapper tr.selected td { color:#2a9be9 !important; font-weight:normal !important; }
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * DataTables features
 */

.dataTables_wrapper {
	position: relative;
	clear: both;
	zoom: 1; /* Feeling sorry for IE */
    z-index: 1;
}

.dataTables_processing {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 250px;
	height: 30px;
	margin-left: -125px;
	margin-top: -15px;
	padding: 14px 0 2px 0;
	border: 1px solid #ddd;
	text-align: center;
	color: #999;
	font-size: 14px;
	background-color: white;
}

.dataTables_length {
	width: 140px;
	float: right;
    height: 20px;
    line-height: 20px;
    vertical-align:middle;
    margin-top: 5px;
	margin-left: 30px;
}
.dataTables_length select { width: 90px; padding-left: 4px;}
.dataTables_length label { font-size: 12px; }

.dataTables_filter {
	width: 50%;
	float: right;
	text-align: right;
}

.dataTables_info {
	/*width: 30%;*/
    width: 244px;
	float: left;
    height: 20px;
    line-height: 20px;
    vertical-align:middle;
    margin-top: 13px;
    text-align:center;
}

.dataTables_paginate {
	width: 44px;
	*width: 50px;
	width: auto !important;
	text-align: right;
    /*margin-left: 300px;*/
    /*margin-right: 150px;*/
    margin-top: 10px;
     font-size: 12px;
}

