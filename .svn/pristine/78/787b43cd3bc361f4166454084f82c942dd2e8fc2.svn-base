/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * Misc
 */
.dataTables_scroll {
	clear: both;
}

.dataTables_scrollBody {
	*margin-top: -1px;
}

.top, .bottom {
	padding: 15px;
	background-color: #F5F5F5;
	border: 1px solid #CCCCCC;
}

.top .dataTables_info {
	float: none;
}

.clear {
	clear: both;
}

.dataTables_empty {
	text-align: center;
}

td.center { text-align: center;}
td.left { text-align:left; padding-left: 10px}
td.right { text-align:right; padding-right: 0px;}

.paging_input, .paging_full_numbers, .paging_inputshort { width: 400px; height: 26px; line-height: 26px; vertical-align:middle; min-height:0; }

.paging_input .paginate_button, .paging_full_numbers .paginate_button, .paging_inputshort span.paginate_button,
.paging_input .paginate_active, .paging_full_numbers .paginate_active, .paging_inputshort span.paginate_button {
	border: 1px solid #ddd;
	padding: 4px 12px;
    margin: 0 3px;
	line-height: 20px;
	cursor: pointer;   
    text-decoration:none;
    *padding: 4px 6px 4px 6px !important;
}

.paging_input .first,.paging_input .previous,
.paging_input .next,.paging_input .last,
.paging_full_numbers .first,.paging_full_numbers .previous,
.paging_full_numbers .next,.paging_full_numbers .last,
.paging_inputshort .first,.paging_inputshort .previous,
.paging_inputshort .next,.paging_inputshort .last {
    padding: 4px 6px !important;
    *padding: 4px 6px 4px 6px !important;
}
.paging_input a.paginate_button,
.paging_full_numbers a.paginate_button,
.paging_inputshort span.paginate_button  {
	background-color: #ffffff;
    color: #333;
}
.paging_input a.paginate_button:hover,
.paging_full_numbers a.paginate_button:hover,
.paging_inputshort span.paginate_button:hover {
	background-color: #eaedf6;
}
.paging_input span.paginate_active,
.paging_full_numbers span.paginate_active,
.paging_inputshort span.paginate_active {
	background-color: #f5f5f5;
    color:#999;
}
a.paginate_button_disabled {
	/*background-color: transparent !important;*/
    color:#333 !important;
}
.paginate_of,paginate_page { border: 0px !important; padding-left: 0px !important;}